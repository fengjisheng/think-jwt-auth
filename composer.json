{
    "name": "fengjisheng/think-jwt",
    "description": "thinkphp  jwt auth composer",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "lcobucci/jwt": "^4.0",
        "nesbot/carbon": "^1.0|^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "fengjisheng",
            "email": "fengjisheng@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "thans\\jwt\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "thans\\jwt\\Service"
            ],
            "config": {
                "jwt": "config/config.php"
            }
        }
    }
}
